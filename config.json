{
  "logger": {
    "root": "/tmp/log/",
    "extension": ".log",
    "level": "debug",
    "pattern": "[%H:%M:%S:%e] [thread %t] [%^%l@%n%$] %v",
    "register_by_default": false,
    "flush_every": 3.0
  },
  "listener": {
    "interface": "127.0.0.1",
    "port": 8080,
    "max_queued_connections": 128
  },
  "number_of_additional_threads": -1
}